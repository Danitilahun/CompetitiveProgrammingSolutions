class Solution:
    def maxOperations(self, nums: List[int], k: int) -> int:
        left=0
        right=len(nums)-1
        operation=0
        nums.sort()
        while left<right:
            cur=nums[left]+nums[right]
            if cur>k:
                right-=1
            elif cur<k:
                left+=1
            else:
                operation+=1
                left+=1
                right-=1
        return operation
